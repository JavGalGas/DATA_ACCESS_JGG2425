<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jgg.unit4.practica1javafx_jgg.SellerDataController">
   <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="120.0" spacing="10.0"
         style="-fx-background-color: linear-gradient(to bottom, #58a05c, #4BBF50, #4BBF50);"
         AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="480.0"
         AnchorPane.topAnchor="0.0">
      <padding>
         <Insets bottom="10.0" top="10.0"/>
      </padding>
      <ImageView fitHeight="80.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@../../../../Images/user-circle-svgrepo-com.png"/>
      </ImageView>
      <ImageView fitHeight="80.0" fitWidth="100.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true"
                 preserveRatio="true">
         <Image url="@../../../../Images/product-f-svgrepo-com.png"/>
      </ImageView>
      <ImageView fitHeight="80.0" fitWidth="100.0" layoutX="10.0" layoutY="110.0" pickOnBounds="true"
                 preserveRatio="true">
         <Image url="@../../../../Images/offer-svgrepo-com.png"/>
      </ImageView>
      <ImageView fitHeight="70.0" fitWidth="100.0" layoutX="10.0" layoutY="210.0" pickOnBounds="true"
                 preserveRatio="true">
         <VBox.margin>
            <Insets left="10.0" top="10.0"/>
         </VBox.margin>
         <Image url="@../../../../Images/exit-svgrepo-com.png"/>
      </ImageView>
   </VBox>
   <VBox alignment="CENTER" prefHeight="400.0" prefWidth="480.0" spacing="35.0" style="-fx-background-color: #547556;"
         AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="0.0"
         AnchorPane.topAnchor="0.0">
      <padding>
         <Insets bottom="10.0"/>
      </padding>
      <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Seller Data" textAlignment="CENTER">
         <VBox.margin>
            <Insets/>
         </VBox.margin>
         <font>
            <Font name="Berlin Sans FB" size="36.0"/>
         </font>
      </Text>
      <Label contentDisplay="RIGHT" graphicTextGap="60.0" prefHeight="25.0" prefWidth="480.0" text="Product: "
             textFill="WHITE">
         <graphic>
            <ComboBox prefWidth="300.0"/>
         </graphic>
         <padding>
            <Insets left="20.0"/>
         </padding>
         <font>
            <Font name="Broadway" size="18.0"/>
         </font>
      </Label>
      <Label contentDisplay="RIGHT" graphicTextGap="86.0" layoutX="159.0" layoutY="65.0" prefWidth="480.0"
             text="From: " textFill="WHITE">
         <graphic>
            <DatePicker prefWidth="300.0"/>
         </graphic>
         <font>
            <Font name="Broadway" size="18.0"/>
         </font>
         <padding>
            <Insets left="20.0"/>
         </padding>
      </Label>
      <Label contentDisplay="RIGHT" graphicTextGap="116.0" layoutX="159.0" layoutY="90.0" prefWidth="480.0"
             text="To: " textFill="WHITE">
         <graphic>
            <DatePicker prefWidth="300.0"/>
         </graphic>
         <font>
            <Font name="Broadway" size="18.0"/>
         </font>
         <padding>
            <Insets left="20.0"/>
         </padding>
      </Label>
      <Label contentDisplay="RIGHT" graphicTextGap="55.0" layoutX="159.0" layoutY="115.0" prefWidth="480.0"
             text="Discount: " textFill="WHITE">
         <graphic>
            <TextField prefWidth="300.0"/>
         </graphic>
         <padding>
            <Insets left="20.0"/>
         </padding>
         <font>
            <Font name="Broadway" size="18.0"/>
         </font>
      </Label>
      <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
              style="-fx-background-color: linear-gradient(to bottom, #4BBF50, #58A05C, #4BBF50);" text="Ok"
              textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Broadway" size="14.0"/>
         </font>
      </Button>
   </VBox>
</AnchorPane>
